<?xml version="1.0" encoding="utf-8"?>
<Objects 
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns="http://www.spicefactory.org/parsley"
    xmlns:parsley="com.bluecallisto.parsley.*"
    >
    <fx:Script>
        <![CDATA[
            import com.bluecallisto.parsley.HelloWorld;
        ]]>
    </fx:Script>
    
    <fx:Declarations>
        
        <Object type="{HelloWorld}">
            <Property name="message" value="Hello world!" />
        </Object>
        <parsley:HelloParsley id="mxmlHelloParsley" />
        
        <!-- could also be:
        
        <parsley:HelloWorld message="Hello world!" />
        <Object type="{HelloParsley}" id="mxmlHelloParsley" />
        
        -->
        
    </fx:Declarations>
    
</Objects>